#include <bits/stdc++.h>
using namespace std;

struct ListNode {
	int val;
	ListNode* next;
	ListNode(int x) {
		val = x;
		next = NULL;
	}
};

int main()
{
	ListNode* head = new ListNode(1);
	ListNode* temp1 = new ListNode(1);
	ListNode* temp2 = new ListNode(0);
	ListNode* temp3 = new ListNode(1);
	head->next = temp1;
	temp1->next = temp2;
	temp2->next = temp3;

	
	return 0;
}
